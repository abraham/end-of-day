{
  "name": "end-of-day",
  "version": "0.1.0",
  "description": "Slack bot for collecting a summary of your team's activity for the day",
  "scripts": {
    "build": "tsc",
    "deploy": "firebase deploy",
    "fix:eslint": "eslint . --ext .ts --fix",
    "fix:prettier": "prettier . --write",
    "fix": "concurrently npm:fix:*",
    "lint:eslint": "eslint . --ext .ts",
    "lint:prettier": "prettier . --check",
    "lint": "concurrently npm:lint:*",
    "postinstall": "npm --prefix functions install",
    "start:tests": "jest --watch",
    "test": "jest"
  },
  "engines": {
    "node": "10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abraham/end-of-day.git"
  },
  "keywords": [
    "slack",
    "team",
    "standup",
    "firebase"
  ],
  "author": "Abraham Williams <abraham@abrah.am>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/abraham/end-of-day/issues"
  },
  "homepage": "https://github.com/abraham/end-of-day#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "26.0.3",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "3.5.0",
    "concurrently": "5.2.0",
    "eslint": "7.3.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-jest": "23.17.1",
    "firebase-tools": "8.4.3",
    "jest": "26.1.0",
    "prettier": "2.0.5",
    "ts-jest": "26.1.1",
    "typescript": "3.9.6"
  },
  "private": true
}
